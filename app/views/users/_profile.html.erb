<h1>User info</h1>
<%= image_tag user.get_profile_image(100,100) %>
<table>
    <tr>
        <td>name</td>
        <td><%= user.name %></td>
    <tr>
    <tr>
        <td>introduction</td>
        <td><%= user.introduction %></td>
    </tr>
    <%= link_to edit_user_path(user.id) do %>
        <span class="fa-solid fa-user-cog"></span>
    <% end %>
</table>